    "start:client": "ng serve client --hmr -o",
    "start:prod": "node apps/api/main",
    "start:server": "nx serve api",
    "test": "nx test",
    "ts-node": "ts-node --compiler-options '{\"module\":\"CommonJS\"}'",
    "update": "nx migrate latest",
